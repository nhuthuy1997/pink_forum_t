<% provide :title, "Danh sách chuyên mục"%>

<%= link_to "Tạo chuyên mục", new_admins_category_path,
  class: "btn btn-danger pull-right m-l-20 hidden-xs hidden-sm waves-effect waves-light" %>
<div class="clearfix margin-bottom-20"></div>
<% if @categories.present? %>
  <%= render "admins/categories/categories_table", categories: @categories %>
<% else %>
  <p>Không có chuyên mục nào</p>
<% end %>
